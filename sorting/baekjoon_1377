# 시간초과

import sys
input = sys.stdin.readline

n = int(input())
array = [int(input()) for _ in range(n)]

flg = False

for i in range(1, n+2):
    flg = False
    for j in range(n-i):
        if array[j] > array[j+1]:
            flg = True
            array[j], array[j+1] = array[j+1], array[j]
    if flg == False:
        print(i)
        break
