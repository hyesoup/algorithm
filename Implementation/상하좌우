from collections import deque
n = int(input())
plans = input().split()
x,y = 1, 1

dx = [-1, 1, 0, 0]
dy = [0, 0, -1, 1]
move_types = ['U', 'D', 'L', 'R']

for plan in plans:
    for i in range(4):
        if plan == move_types[i]:
            nx = x + dx[i]
            ny = y + dy[i]

    if 0<nx<=n and 0<ny<=n:
        x,y = nx,ny

print(x,y)
