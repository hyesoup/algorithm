# 가장 빠른, 가장 짧은 => BFS


from collections import deque
n, k = map(int,input().split())
visited = [0]*100001

def bfs():
    queue = deque()
    queue.append(n)

    while queue:
        x = queue.popleft()

        if x==k:
            return print(visited[x])

        for i in (x-1, x+1, 2*x):
            if 0 <= i <= 100000 and not visited[i]:
                queue.append(i)
                visited[i] = visited[x]+1


bfs()
