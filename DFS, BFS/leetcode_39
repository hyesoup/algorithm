# DFS, 중복조합, 백트래킹

candidates = [2,3,6,7]
target = 7

answer = []
def dfs(csum, index, path):
    if csum < 0:
        return
    elif csum == 0:
        answer.append(path)

    for i in range(index, len(candidates)):
        dfs(csum-candidates[i], i, path+[candidates[i]])

dfs(target, 0, [])
print(answer)
