# permutation 라이브러리를 이용한 풀이

from itertools import permutations
input = [1,2,3]
answer = []
for i in permutations(input, len(input)):
    answer.append(list(i))
print(answer)

# leet
from itertools import permutations
class Solution:
    def permute(self, nums: List[int]) -> List[List[int]]:
        answer = []
        for i in permutations(nums, len(nums)):
            answer.append(list(i))
        return answer

